{
  "name": "@typhonjs-fvtt/svelte",
  "version": "0.0.0",
  "description": "Provides resources to integrate Svelte w/ Foundry VTT.",
  "type": "module",
  "author": "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)",
  "contributors": [
    "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)"
  ],
  "svelte": "src/modules/index.js",
  "exports": {
    ".": {
      "import": "./dist/modules/index.js"
    },
    "./gsap": {
      "import": "./dist/modules/gsap.js"
    },
    "./helpers": {
      "import": "./dist/modules/helpers.js"
    },
    "./store": {
      "import": "./dist/modules/store.js"
    },
    "./plugins/data": {
      "import": "./dist/plugins/data.js"
    },
    "./plugins/system": {
      "import": "./dist/plugins/system.js"
    },
    "./styles/*": "./dist/styles/*",
    "./package.json": "./package.json"
  },
  "imports": {
    "#stylesAppShell": {
      "default": "./dist/styles/application-shell.css"
    },
    "#stylesIndex": {
      "default": "./dist/modules/styles/index.css"
    }
  },
  "dependencies": {
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@rollup/plugin-babel": "^5",
    "@rollup/plugin-node-resolve": "^13",
    "@rollup/plugin-virtual": "^2",
    "@typhonjs-config/eslint-config": "^0.3.0",
    "@typhonjs-fvtt/eslint-config-foundry.js": "^0.8.0",
    "autoprefixer": "^10",
    "eslint": "^8",
    "postcss": "^8",
    "postcss-preset-env": "^6",
    "rollup": "^2",
    "rollup-plugin-postcss": "^4",
    "rollup-plugin-sourcemaps": "^0.6.0",
    "rollup-plugin-svelte": "^7",
    "rollup-plugin-terser": "^7",
    "sass": "^1",
    "svelte": "^3",
    "svelte-persistent-store": "github:typhonjs-svelte/svelte-persistent-store"
  },
  "engines": {
    "node": ">=12"
  },
  "homepage": "https://github.com/typhonjs-fvtt/svelte#readme",
  "keywords": [
    "foundry vtt",
    "fvtt",
    "svelte"
  ],
  "scripts": {
    "build": "rollup --config",
    "eslint": "eslint ."
  },
  "files": [
    "/dist",
    "/src"
  ],
  "directories": {},
  "license": "MPL-2.0",
  "repository": "github:typhonjs-fvtt/svelte",
  "bugs": "https://github.com/typhonjs-fvtt/svelte/issues"
}
